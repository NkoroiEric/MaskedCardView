<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaskedCardView">
        <attr name="cornerSide" format="enum">
            <enum name="bottomLeft_topRight" value="1"/>
            <enum name="bottomRight_topLeft" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr name="edgeSide" format="enum">
            <enum name="right_left" value="1"/>
            <enum name="top_bottom" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr name="cornerFamilyType" format="enum">
            <enum name="rounded" value="0"/>
            <enum name="cut" value="1"/>
        </attr>
        <attr name="cornerSizeDp" format="dimension"/>
        <attr name="edgeSizeDp" format="dimension"/>
    </declare-styleable>
</resources>